{
  
  "openapi": "3.1.0",
  "info": {
    "title": "3rdwWave OpenApi Documentation",
    "description": "A sample API that allowes the user to get access to 3rdwave data",
    "license": {
      "name": "MIT"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080/blinco-3ww-webapp/api/public/"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "paths": {
    "/getTransportEventDate": {
      "post": {
        "description": "Query transport events info based on the location codes, transport unit label and transport event type",
        "requestBody": {
          "description": "Transport Event Request to pass to the api",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/transportEventRequestDto"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "transport event response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/transportEventResponseDto"
                }
              }
            }
          },
          "400": {
            "description": "unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/getActualDate": {
      "post": {
        "description": "Query transport events info based on the location codes, transport unit label and transport event type",
        "requestBody": {
          "description": "Transport Event Request to pass to the api",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/transportEventRequestDto"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "transport event response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/transportEventResponseDto"
                }
              }
            }
          },
          "400": {
            "description": "unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "transportEventRequestDto": {
        "type": "object",
        "properties": {
            "locationCode":{
              "description":"The port Code",
              "type":"string",
              "example":"USLGB"
            },
            "transportUnitLabel":{
              "description":"The transport unit label",
              "type":"string",
              "example":"ABTU12345"
            },
            "transportEventType":{
              "description":"The transport event type",
              "type":"string",
              "example":"ETA"
            }
        }
      },
      "transportEventResponseDto": {
        "type": "object",
        "properties": {
            "transportUnitNumber":{
              "description":"Transport Unit Number",
              "type":"string",
              "example":"ABTU12345"
            },
            "transportUnitReference":{
              "description":"The transport unit reference",
              "type":"string",
              "example":"CNT000001"
            },
            "eventDate":{
              "description":"Event Date",
              "type":"string",
              "format": "date",
              "example":"2025-01-01"
            },
            "transportEventType":{
              "description":"The transport event type",
              "type":"string",
              "example":"ETA"
            }
        }
      },
      "Error": {
        "required": [
          "error",
          "message"
        ],
        "type": "object",
        "properties": {
          "error": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}