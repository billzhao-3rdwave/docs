  "paths": {
    "/getTransportEventDate": {
      "post": {
        "tags": ["Transportation"],
        "operationId": "getTransportEventDate",
        "description": "Query transport events info based on location codes, transport unit label, and event category",
        "requestBody": {
          "description": "Transport Event Request",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/transportEventRequestDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Transport event response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/transportEventResponsesDto"
                }
              }
            }
          },
          "400": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/getActualDeliveryDate": {
      "post": {
        "tags": ["Transportation"],
        "operationId": "getActualDeliveryDate",
        "description": "Query actual delivery date based on transport unit and location",
        "requestBody": {
          "description": "Transport Unit Node Request",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/transportUnitNodeRequestDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Transport unit node response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/transportUnitNodeResponsesDto"
                }
              }
            }
          },
          "204": {
            "description": "No content found"
          },
          "400": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    }
  },
